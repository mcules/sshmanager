<?xml version="1.0" encoding="UTF-8"?>
<sshmanager>
	<keys>
		<!--
			Personal keys
		-->
		<key alias="schlepptopp">
ssh-rsa  josef.stautner@r-kom.de
		</key>

		<key alias="pc">
ssh-rsa  veloc1ty@pc
		</key>

		<key alias="s4">
ssh-rsa  key
		</key>

		<!--
			Server user keys
		-->
		<key alias="currywurstRoot">
ssh-rsa  root@currywurst
		</key>

		<key alias="speziRoot">
ssh-rsa  root@spezi
		</key>

		<key alias="piRoot">
ssh-rsa  root@pi
		</key>
		
	</keys>

	<groups>
		<!--
			This is my group where I only add my personal keys
			which were not generated by another programm as
			OpenSSH itself ( e.g.: Android ConnectBot, JuiceSSH, etc)
			Call me paranoid but I don't trust them!
		-->
		<group alias="personalSensible">
			<keys>
				<key>pc</key>
				<key>schlepptopp</key>
			</keys>
		</group>

		<group alias="personal">
			<keys>
				<key>pc</key>
				<key>schlepptopp</key>
				<key>s4</key>
			</keys>
		</group>

		<group alias="gitAccess">
			<keys>
				<key>currywurstRoot</key>
				<key>speziRoot</key>
				<key>piRoot</key>
				<key>pc</key>
				<key>schlepptopp</key>
			</keys>
		</group>
	</groups>

	<hosts>

		<!--
			Machine: currywurst.veloc1ty.de
		-->
		<host name="currywurst">
			<users>

				<user name="root">
					<groups>
						<group>personal</group>
					</groups>
				</user>

				<user name="ca">
					<groups>
						<group>personalSensible</group>
					</groups>
				</user>

				<user name="git">
					<groups>
						<group>personal</group>
						<group>gitAccess</group>
					</groups>
				</user>

				<user name="insurgency">
					<groups>
						<group>personal</group>
					</groups>
				</user>

				<user name="proxycheck">
					<groups>
						<group>personal</group>
					</groups>
				</user>

				<user name="teamspeak">
					<groups>
						<group>personal</group>
					</groups>
				</user>

				<user name="veloc1ty">
					<groups>
						<group>personal</group>
					</groups>
				</user>

			</users>
		</host>

		<!--
			Machine: schlepptopp.veloc1ty.lan
		-->
		<host name="schlepptop">
			<users>

				<user name="root">
					<groups>
						<group>personal</group>
					</groups>
				</user>

				<user name="josef">
					<groups>
						<group>personal</group>
					</groups>
				</user>
				
			</users>
		</host>

		<!--
			Machine: laptop.veloc1ty.lan
		-->
		<host name="laptop">
			<users>

				<user name="root">
					<groups>
						<group>personal</group>
					</groups>
				</user>

				<user name="veloc1ty">
					<groups>
						<group>personal</group>
					</groups>
				</user>

			</users>
		</host>

		<!--
			Machine: pi.veloc1ty.lan
		-->
		<host name="pi">
			<users>

				<user name="root">
					<groups>
						<group>personal</group>
					</groups>
				</user>

			</users>
		</host>

	</hosts>
</sshmanager>